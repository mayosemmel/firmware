+-	/* mt7621 doesn't have txcsum config */
+-	mt7621_rxcsum_config((dev->features & NETIF_F_RXCSUM));
+-	mt7621_rxvlan_config(priv->flags & FE_FLAG_RX_VLAN_CTAG);
