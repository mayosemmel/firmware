+-	if (of_property_read_string(np, "mediatek,portmap", &map))
+-		return NULL;
+-
+-	for (i = 0; i < ARRAY_SIZE(mt7530_defaults); i++)
+-		if (!strcmp(map, mt7530_defaults[i].name))
+-			return &mt7530_defaults[i];
+-
+-	return NULL;
