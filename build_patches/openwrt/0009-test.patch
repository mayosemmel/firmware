+-	ring->rx_max_pending = MAX_DMA_DESC;
+-	ring->tx_max_pending = MAX_DMA_DESC;
+-	ring->rx_pending = priv->rx_ring.rx_ring_size;
+-	ring->tx_pending = priv->tx_ring.tx_ring_size;
