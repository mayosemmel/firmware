+-	esw_rmw(esw,
+-		       RT305X_ESW_REG_VLANI(vlan / 2),
+-		       RT305X_ESW_VLANI_VID_M << s,
+-		       (vid & RT305X_ESW_VLANI_VID_M) << s);
+-}
