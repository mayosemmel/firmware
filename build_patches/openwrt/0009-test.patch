+-static int
+-mt7530_get_port_pvid(struct switch_dev *dev, int port, int *val)
+-{
+-	struct mt7530_priv *priv = container_of(dev, struct mt7530_priv, swdev);
+-
+-	if (port >= MT7530_NUM_PORTS)
+-		return -EINVAL;
+-
+-	*val = mt7530_r32(priv, REG_ESW_PORT_PPBV1(port));
+-	*val &= 0xfff;
+-
+-	return 0;
+-}
