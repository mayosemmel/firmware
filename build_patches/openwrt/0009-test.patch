+-static void fe_get_strings(struct net_device *dev, u32 stringset, u8 *data)
+-{
+-	switch (stringset) {
+-	case ETH_SS_STATS:
+-		memcpy(data, *fe_gdma_str, sizeof(fe_gdma_str));
+-		break;
+-	}
+-}
